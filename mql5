//+------------------------------------------------------------------+
//| Expert tick function                                             |
//+------------------------------------------------------------------+
void OnTick()
{
    // Analyze the Stochastic line on the last closed candle

    if (StochasticLineAboveLevel()) 
    {
        GenerateSellSignal();
    }
    else if (StochasticLineBelowLevel()) 
    {
        GenerateBuySignal();
    }
}

